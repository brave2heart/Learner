<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wh.customcontrol">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.jweihao.jdemo.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.jweihao.jdemo.ui.activity.custom.Level3MenuActivity" />
        <activity android:name="com.jweihao.jdemo.ui.activity.custom.ColorTextActivity" />
        <activity android:name="com.jweihao.jdemo.ui.activity.custom.DragFramLayoutActivity" />
        <activity android:name="com.jweihao.jdemo.ui.activity.custom.DragSquareActivity" />
        <activity android:name="com.jweihao.jdemo.ui.activity.custom.MeasureActivity" />
        <activity android:name="com.jweihao.jdemo.ui.activity.custom.ShineTextViewActivity" />
        <activity android:name="com.jweihao.jdemo.ui.activity.custom.JToolBarActivity" />
        <activity android:name="com.jweihao.jdemo.ui.activity.custom.CustomActivity"></activity>
    </application>

</manifest>